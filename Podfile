# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Travis-Firebase-iOS-Example' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Travis-Firebase-iOS-Example
  pod 'Firebase/Database'
  pod 'Firebase/Auth'

  target 'Travis-Firebase-iOS-ExampleTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase/Database'
    pod 'Firebase/Auth'
  end

  target 'Travis-Firebase-iOS-ExampleUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase/Database'
    pod 'Firebase/Auth'
  end

end
